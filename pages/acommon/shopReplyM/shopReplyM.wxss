.cardWrap{
  background: #fff;
  padding: 25rpx 14rpx 29rpx 24rpx;
  box-shadow:0px 1rpx 14rpx 0px rgba(0,22,48,0.05);
  border-radius:8rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  margin-bottom: 24rpx;
}

/* 上 */

.titleWrap{
  border-bottom:1rpx solid rgba(226,226,226,1);
  padding-bottom: 7rpx;
}

.title{
  display: flex;
  font-size:28rpx;
  color:#242526;
  justify-content: space-between;
  align-items: center;
  padding: 0rpx 0rpx 20rpx 0rpx;
}

.number{
  font-weight:bold;
  font-size:32rpx;
  color:#242526;
  padding-left: 18rpx;
  border-left:6rpx solid #BF1D47;
  height:32rpx;
  line-height: 32rpx;

}

.status{
  font-size:28rpx;
  font-weight:400;
  color:#FF8C19
}


/* 中 */
.middleWrap{
  padding: 36rpx 0rpx 32rpx 0rpx;
  border-bottom:1rpx solid rgba(226,226,226,1);
}

.middle{
  padding: 0rpx 19rpx 0rpx 12rpx;
  font-size:28rpx;
  font-weight:400;
  color:rgba(51,51,51,1);
}

.tel{
  margin-top: 10rpx;
}

/* 底部 */
.bottomWrap{
   padding: 18rpx 19rpx 0rpx 12rpx;
}

.time{
  font-size:24rpx;
  font-weight:400;
  color:rgba(153,153,153,1);
 
}


/* 按钮 */
.btnWrap{
  text-align: right;
}

.customBtn{
  width:129rpx;
  height:49rpx;
  background:rgba(225,103,29,1);
  border-radius:2rpx;
  font-size:26rpx;
  font-weight:400;
  color:rgba(255,255,255,1);
  letter-spacing:1rpx;
  text-align: center;
  line-height: 49rpx;
  margin-top: 6rpx;
  display: inline-block;
}

/* 文字 */

.detailWrap{
  text-align: right;
}

.detailDes{
  font-size:26rpx;
  font-weight:400;
  color:rgba(225,103,29,1);
  letter-spacing:1rpx;
  margin-top: 20rpx;
}

.rightArrow{
  font-size:24rpx;
}

.displayFlex{
  display:flex;
}
