.feedbackWrap{
  background: #fff;
  padding: 33rpx 9rpx 72rpx 9rpx;
  font-family:PingFangSC-Medium,PingFang SC; 
  margin-bottom: 25rpx;
}

.feedbackTitleWrap{
  border-bottom:1rpx solid rgba(224,224,224,1);
  padding-bottom: 21rpx;
}

.feedbackTitle{
  font-size:30rpx;
  font-weight:500;
  color:rgba(0,0,0,1);
  letter-spacing:1rpx;
  margin-left: 27rpx;
}

/* 评价 */
.assessWrap{
  padding: 39rpx 0rpx 50rpx 0rpx;
  border-bottom:1rpx solid rgba(224,224,224,1);
}

.assess{
  margin-left: 27rpx;
}

.assessLi{
  display: flex;
  align-items: center;
  margin-bottom: 30rpx;
 
}

.assessLi:last-child{
  margin-bottom: 0rpx;
}

.left{
  font-size:28rpx;
  font-weight:400;
  color:rgba(102,102,102,1);
  letter-spacing:1px;
  flex: 1;
}

.right{
  flex: 1;
  display: flex;
  justify-content: space-between;

}

.star{
  color: #BCBCC7;
  font-size: 40rpx;
}

.real{
  color: #E1751D;
}

/* 天数 */
.dayWrap{
  padding: 30rpx 0rpx;
  border-bottom:1rpx solid rgba(224,224,224,1);
}

.day{
  padding: 0rpx 15rpx 0rpx 27rpx;
  display: flex;
  justify-content: space-between;
}

.dayLeft{
  font-size:28rpx;
  font-weight:400;
  color:rgba(102,102,102,1);
  letter-spacing:1rpx;
}

.dayRight{
  font-size:28rpx;
  font-weight:400;
  color:rgba(0,0,0,1);
  letter-spacing:1rpx;
}

/* 巡店建议 */

.proposalWrap{
  padding: 25rpx 15rpx 0rpx 27rpx;
  display: flex;
}

.proposalWrapTitle{
  font-size:28rpx;
  font-weight:400;
  color:rgba(102,102,102,1);
  letter-spacing:1rpx;
  flex-shrink:0;
  margin-right: 40rpx;
}

.proposalWrapDes{
  font-size:24rpx;
  font-weight:400;
  color:rgba(102,102,102,1);
  flex: 1;
  line-height:36rpx;
}