/* 导航 */
.nav{
  display: flex;
  align-items: center;
  justify-items: center;
  height:97rpx;
  background:rgba(255,255,255,1);
  font-size:28rpx;
  border-top:1rpx solid #F2F2F2;
}

.navLi{
  flex: 1;
  font-size:28rpx;
